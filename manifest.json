{
  "name": "Download Video from sites",
  "version": "0.1.0",
  "description": "Download video from Websites",
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*/*","http://*/*"],
      "js": ["jquery1.js","content.js"],
      "all_frames": true
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/ext-icon.png",
      "24": "assets/ext-icon.png",
      "32": "assets/ext-icon.png"
    },
    "default_title": "My YT Bookmarks",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}